;
; BIND reverse data file for local loopback interface
;
$TTL    604800
@       IN      SOA     ns1.mandialand.com. admin.mandialand.com. (
                              3         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
; name servers - NS records
      IN      NS      ns1.mandialand.com.

; PTR Records
101   IN      PTR     ns1.mandialand.com.         ; 192.168.200.101
150   IN      PTR     mandialand.com.  ; 192.168.200.152
151   IN      PTR     prometheus.mandialand.com.  ; 192.168.200.151
152   IN      PTR     grafana.mandialand.com.  ; 192.168.200.152
153   IN      PTR     kibana.mandialand.com.  ; 192.168.200.153
154   IN      PTR     jenkins.mandialand.com.  ; 192.168.200.152
155   IN      PTR     argocd.mandialand.com.  ; 192.168.200.152
